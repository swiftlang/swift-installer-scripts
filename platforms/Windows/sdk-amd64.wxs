                  Return="check" />

    <!-- Hooks -->
    <InstallExecuteSequence>
      <Custom Action="SwiftInstaller_InstallAuxiliaryFiles.SetProperty"
              Before="SwiftInstaller_InstallAuxiliaryFiles" />

      <Custom Action="SwiftInstaller_InstallAuxiliaryFiles" After="InstallExecute">
