    <?define PlatformRoot = "$(ImageRoot)\Platforms\Windows.platform"?>
    <?define SDKRoot = "$(PlatformRoot)\Developer\SDKs\Windows.sdk"?>

    <Media Id="5" Cabinet="windows.cab" EmbedCab="$(ArePackageCabsEmbedded)" />
    <?if $(IncludeARM64) = True?>
      <Media Id="6" Cabinet="sdk.windows.arm64.cab" EmbedCab="$(ArePackageCabsEmbedded)" />
    <?endif?>
    <?if $(IncludeX64) = True?>
      <Media Id="7" Cabinet="sdk.windows.x64.cab" EmbedCab="$(ArePackageCabsEmbedded)" />
    <?endif?>
    <?if $(IncludeX86) = True?>
      <Media Id="8" Cabinet="sdk.windows.x86.cab" EmbedCab="$(ArePackageCabsEmbedded)" />
    <?endif?>

    <WixVariable Id="SideBySidePackageUpgradeCode" Value="$(WindowsPlatformUpgradeCode)" />
    <FeatureGroupRef Id="SideBySideUpgradeStrategy" />

