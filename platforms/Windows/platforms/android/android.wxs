        </Component>

        <Component Directory="XCTest_usr_lib_swift_android_x86" DiskId="5">
          <File Source="$(PlatformRoot)\Developer\Library\XCTest-$(ProductVersion)\usr\lib\swift\android\i686\libXCTest.so" />
        </Component>
      </ComponentGroup>
    <?endif?>

    <!-- Testing -->
